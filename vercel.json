{
  "version": 2,
  "framework": null,
  "functions": {
    "server.js": {
      "maxDuration": 60,
      "includeFiles": "public/**"
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
